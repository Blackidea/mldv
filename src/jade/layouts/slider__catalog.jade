ul.sliderHotelsAround.sliderHotelsAround__list.sliderHotelsAround__list--tiles-view
  li.sliderHotelsAround__list-item
    .card
      a(href='#')
        .card__img-box
          img(src='images/card-image/card-image-06.jpg', alt='')
          span.card__rating-icon 7.3
            //- img(src="./images/card-image/icon-rating.png", alt="")
