//category card ----------------------
.category-card
  position: relative
  height: 348px
  background: #000
  overflow: hidden
  border-radius: 10px
  box-shadow: 0 0 3.8px 0.2px rgba(35, 41, 50, 0.3)
  text-align: center
  &:before
    content: ''
    display: inline-block
    height: 100%
    vertical-align: middle
  &:hover
    img
      opacity: .3
      transform: scale(1.05)
    .category-card__count
      opacity: 1
  a
    text-decoration: none
  &__title
    color: #fff
    font-size: 22px
    font-weight: 700
    position: relative
    display: inline-block
    line-height: 22px
    top: 3px
    position: relative
    width: 62%
  &__count
    display: block
    position: absolute
    top: 100%
    left: 0
    width: 100%
    opacity: 0
    transition: opacity .2s ease
  img
    position: absolute
    opacity: .8
    top: 0
    left: 0
    min-height: 100%
    min-width: 100%
    transition: all .4s ease
//category card end ------------------
//news ------------------
.news
  &__item
    border-radius: 10px
    box-shadow: 0 0 3.8px 0.2px rgba(35, 41, 50, 0.3)
    overflow: hidden
  &__img-box
    height: 205px
    overflow: hidden
    img
      // min-width: 100%;
      // min-height: 100%;
  &__content
    padding: 11px 9px 8px 12px
    clearfix()
  &__date
    color: #b3adad
    font-size: 12px
    line-height: 14px
    display: block
    padding: 0 0 4px 0
  &__title
    display: block
    height: 38px
    overflow: hidden
    padding: 0 0 8px 0
    a
      font-weight: 700
      line-height: 15px
      color: $mainFontColor
      text-decoration: none
  &__description
    padding: 0
    height: 62px
    padding: 0 0 2px 0
    overflow: hidden
  &__link
    float: right
  b(mobile)
  b(mobile)
//news end -------------------
//feedback ---------------
.feedback
  &__item
    border-radius: 10px
    box-shadow: 0 0 3.8px 0.2px rgba(35, 41, 50, 0.3)
