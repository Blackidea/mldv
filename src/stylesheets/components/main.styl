// @import 'sprite.scss'
// @import 'normalize&reset.scss'
@import '../base/mixins&includes.styl'
// main
//----------------------------------------------------//
.main
  overflow: hidden
  padding: 0 3px 0 30px
  &.sidebar-is-fixed
    margin-left: 264px
  +b(tablet)
    margin: 0;
    padding: 0;
// catalog-page
//----------------------------------------------------//
.catalog-page
  .container
    position: relative
// helpers
//----------------------------------------------------//
.tbl
  display: table
.cell
  display: table-cell
  vertical-align: middle
  text-align: center
// icons
//----------------------------------------------------//
.icon-plane-up
  width: 29px
  height: 14px
  fill: #24b3fd
.icon-pen
  width: 20px
  height: 20px
  fill: #24b3fd
.icon-arrow-direction
  width: 31px
  height: 12px
  fill: #7a7a7a
.icon-arrow-left
  width: 7px
  height: 13px
  fill: #748893
.icon-arrow-right
  width: 7px
  height: 13px
  fill: #748893
.icon-star
  width: 23px
  height: 22px
  fill: #aeaeae
.icon-star-fill
  width: 23px
  height: 22px
  fill: #24b3fd
.icon-no-food
  width: 17px
  height: 17px
  fill: #6a6a6a
.icon-breakfest
  width: 15px
  height: 19px
  fill: #6a6a6a
.icon-lunch-alternative
  width: 21px
  height: 17px
  fill: #6a6a6a
.icon-lunch
  width: 18px
  height: 16px
  fill: #6a6a6a
.icon-coctail
  width: 12px
  height: 16px
  fill: #6a6a6a
.icon-tour
  width: 55px
  height: 44px
  fill: #fff
.icon-hotel
  width: 43px
  height: 43px
  fill: #fff
.icon-plane
  width: 65px
  height: 37px
  fill: #fff
.icon-compass
  width: 24px
  height: 24px
  fill: #24b3fd
.icon-people
  width: 24px
  height: 22px
  fill: #24b3fd
.icon-calendar
  width: 21px
  height: 21px
  fill: #24b3fd
.icon-location
  width: 22px
  height: 22px
  fill: #24b3fd
.icon-man
  width: 19px
  height: 44px
  fill: #24b3fd
.icon-baby
  width: 21px
  height: 37px
  fill: #24b3fd
.icon-minus
  width: 20px
  height: 4px
  stroke: #dcdcdc
.icon-plus
  width: 20px
  height: 20px
  stroke: #dcdcdc
// back
//----------------------------------------------------//
.back
  display: block
  background-color: #f4f4f4
  text-decoration: none
  color: #677e8a
  font-size: 16px
  position: relative
  z-index: 10
  padding: 12px 15px
  [class^="icon-"],
  [class*="icon-"]
    vertical-align: middle
    margin-right: 12px
    margin-top: -3px
// sidebar
//----------------------------------------------------//
.sidebar
  margin-bottom: 25px
  width: 264px
  min-height: 1%
  float: left
  box-shadow: 0px 0px 3.8px 0.2px rgba(35, 41, 50, 0.3)
  border-radius: 8px
  overflow: hidden
  background-color: #fff
  &.to-top
    position: fixed
    top: 50px
    z-index: 1
  &.to-bottom
    position: absolute
    top: auto
    bottom: 0
  +b(tablet)
    //position: absolute;
    position: fixed;
    z-index: 100;
    top: 46px;
    bottom: 0;
    left: 0;
    transition: all 0.2s;
    transform: translateX(-100%);
    width: 100%;
    box-shadow: none;
    border-radius: 0;

    .tabs__list {
      display: none;
    }

    &.active {
      display: block;
      transform: translateX(0);
    }
  +b(mobile)
    //top:0
// tabs
//----------------------------------------------------//
.tabs
  &__list
    display: table
    width: 100%
  &__li
    display: table-cell
    border-right: 1px solid #fff
    &:last-child
      border-right: none
    &.active
      border-right: 0
      .tabs__link
        background-color: #24b3fd
        color: #fff
        font-weight: 700
  &__link
    display: block
    text-align: center
    padding: 10px 12px
    text-transform: uppercase
    text-decoration: none
    background-color: #e8e8e8
    color: #000000
    &:hover
      color: #000000
      background-color: darken(#e8e8e8, 8%)
  &__content
    display: none
    &.active
      display: block
// filter
//----------------------------------------------------//

.filter__box-shadow
  box-shadow 1px 1px 3px #999,
            -1px -1px 3px #999
.filter
  &__back
    display: none
  &__scroll
    // overflow: auto;
    width: 100% !important
  &__inner
    padding: 0 19px
  &__item
    text-align: left
    padding: 16px
    border-bottom: 1px solid #cccccc
    &:last-of-type
      border-bottom: none
  &__title
    display: block
    margin-bottom: 14px
  &__buttons
    text-align: center
    padding: 0 0 10px
  &__button
    min-width: 166px
    display: block
    margin: 0 auto 5px
  &__reset
    color: #000
    font-weight: 600
    text-decoration: none
    &:hover
      color: #24b3fd
  &__range
    padding-top: 3px
  &__stars
    margin-top: -10px
  &__list
    &--inline
      margin-left: -16px
      margin-right: -16px
      .filter__li
        margin-right: 7px
        display: inline-block
        vertical-align: top
        &:last-child
          margin-right: 0
  &__li
    margin-bottom: 4px
  +b(tablet)
    &__back {
      display: block;
    }
// range
//----------------------------------------------------//
.range
  &__slider
    margin-bottom: 7px
  &__values
    clearfix()
    font-weight: 600
  &__value
    &:first-child
      float: left
    &:last-child
      float: right
// ui-slider
//----------------------------------------------------//
.ui-slider
  position: relative
  height: 6px
  background-color: #cccccc
.ui-slider-handle
  position: absolute
  z-index: 2
  width: 14px
  height: 14px
  background-color: #24b3fd
  border-radius: 50%
  border: 2px solid #fff
  cursor: pointer
  top: -4px
  margin-left: -7px
.ui-slider-range
  position: absolute
  z-index: 1
  top: 0
  height: 100%
  background-color: #474747
.ui-slider-range-min
  left: 0
.ui-slider-range-max
  right: 0
// stars
//----------------------------------------------------//
.stars
  input[type="radio"]
    & + label
      padding: 0
      height: 22px
      &:hover
        .icon-star
          fill: #677e8a
      .icon-star-fill
        display: none
      &:before,
      &:after
        display: none !important
  input[type="radio"].checked
    & + label
      .icon-star
        display: none
      .icon-star-fill
        display: block
// scrollPane
//----------------------------------------------------//
.jspVerticalBar
  width: 8px
.jspDrag
  background-color: #555555
  border-radius: 4px
.jspContainer
  width: 100% !important
// slider
//----------------------------------------------------//
.slider
  max-height: 498px
  overflow: hidden
  img
    width: 100%
  +b(mobile)
    max-height: 460px;

    .slick-dots {
      display: none !important;
    }

    img {
      min-height: 460px;
      width: auto;
    }
// slick-dots
//----------------------------------------------------//
.slick-dots
  font-size: 0
  line-height: 0
  position: absolute
  bottom: 0
  width: 100%
  text-align: center
  margin-bottom: 27px
  li
    display: inline-block
    margin: 0 3px
    &.slick-active
      button
        background-color: #24b3fd
  button
    width: 15px
    height: 15px
    font-size: 0
    border-radius: 50%
    padding: 0
    background-color: #fff
// switch
//----------------------------------------------------//
.switch
  &__inner
    background-color: rgba(0, 0, 0, 0.7)
  &__list
    font-size: 0
    text-align: center
    padding: 0 15px
  &__li
    display: inline-block
    vertical-align: top
    &.active
      .switch__link
        font-weight: 700
        background-color: #24b3fd
  &__link
    display: block
    font-size: 22px
    text-transform: uppercase
    color: #fff
    padding: 3px 25px
    text-decoration: none
    min-height: 50px
    transition: background-color 0.8s
    [class^="icon-"],
    [class*="icon-"],
    span
      vertical-align: middle
    [class^="icon-"],
    [class*="icon-"]
      margin-right: 15px
    .icon-plane
      margin-top: 3px
      margin-bottom: 3px
    &:hover
      color: #fff
  &__view
    position: relative
    overflow: hidden
    padding: 0 15px 15px
  &__content
    transform: translateY(-100%)
    transition: all 0.4s
    position: absolute
    width: 100%
    top: 0
    background-color: #fff
    &.active
      transform: translateY(0)
      position: relative
  +b(mobile)
    &__inner {
      margin: 0 -15px;
      background-color: transparent;
    }

    &__list {
      background-color: rgba(0, 0, 0, 0.7);
      padding: 0;
    }

    &__li {
      width: 33.33%;
    }

    &__link {
      min-height: 76px;
      text-transform: none;
      font-size: 16px;

      [class^="icon-"],
      [class*="icon-"] {
        margin-right: 0;
        margin-bottom: 8px;
      }

      span {
        display: block;
      }
    }

    &__view {
      padding: 0;
    }

    &__content {
      background-color: transparent;
    }
// top
//----------------------------------------------------//
.top
  position: relative
  margin: 0 0 30px 0
  &__switch
    position: absolute
    width: 100%
    bottom: 70px

  &__slider .slider__li::after
    content ''
    position absolute
    bottom 0
    left 0
    display block
    width 100%
    height 60%
    background-image linear-gradient(transparent, #021427)
    opacity .58

  +b(mobile)
    &__switch {
      position: relative;
      bottom: auto;
    }

    &__slider {
      position: absolute;
      top: 0;
      bottom: 0;
      left: 0;
      right: 0;
    }
// search
//----------------------------------------------------//
.search
  clearfix()
  position: relative
  &__items
    float: left
    width: 100%
    padding-right: 168px
  &__item
    float: left
    width: 22%
    position: relative
    padding: 6px 0 5px 0
    border-left: 1px solid #dcdcdc
    height: 50px
    &:first-child
      border-left: none
    &--lg
      width: 28%
  &__icon
    cursor: pointer
    position: absolute
    left: 18px
    top: 50%
    transform: translateY(-50%)
  &__arrow
    cursor: pointer
    width: 13px
    height: 7px
    fill: #748893
    position: absolute
    right: 8px
    top: 50%
    transform: translateY(-50%)
  &__button
    float: left
    width: 168px
    margin-left: -168px
    height: 50px
  &__label
    width: 86px
    float: left
    margin-left: -100%
    line-height: 27px
  &__controls
    display: none
    text-align: center
  &__control
    display: block
    margin: 0 auto 5px
    min-width: 180px
    padding-top: 12px
    padding-bottom: 12px
  &__reset
    color: #000000
    text-decoration: none
  &__back
    display: none
    margin: 0 -15px 15px
  &--sm
    padding-top: 10px
    margin-bottom: 13px
    .search__items
      padding-right: 0
      padding-left: 86px
    .search__item
      padding-top: 0
      padding-bottom: 0
      height: auto
      &:last-child
        padding-right: 0
        .search__arrow
          right: 0
    input.search__input
      font-weight: 400
      padding-top: 2px
      padding-bottom: 2px
  +b(mobile)
    background-color: rgba(0, 0, 0, 0.5);
    padding: 15px 15px 0;

    &__items {
      padding-right: 0;
    }

    &__item {
      float: none;
      width: 100%;
      margin-bottom: 16px;
      background-color: #fff;
      border-left: none;

      &--people {
        display: none;
      }
    }

    &__button {
      margin: 0;
      display: block;
      float: none;
      width: 100%;
      font-size: 20px;
      height: auto;
      padding-top: 20px;
      padding-bottom: 20px;
    }

    &__icon {
      left: 10px;

      &.icon-compass {
        width: 18px;
        height: 18px;
      }

      &.icon-calendar {
        width: 19px;
        height: 19px;
      }

      &.icon-geo {
        width: 19px;
        height: 19px;
      }
    }

    &__label {
      display: none;
    }

    &__controls {
      display: block;
    }

    &__back {
      display: block;
    }

    &--lg {
      .people-drop {
        color: #fff;

        [class^="icon-"],
        [class*="icon-"] {
          fill: #fff;
          stroke: #fff;
        }
      }
    }

    &--sm {
      display: none;
      background-color: #fff;
      position: absolute !important;
      z-index: 100;
      display: block !important;
      top: 0 !important;
      left: 0 !important;
      bottom: 0;
      width: 100%;
      transform: translateX(-100%);
      transition: all 0.2s;
      margin-bottom: 0;
      padding-top: 0;

      &.active {
        transform: translateX(0);
      }

      .search__items {
        padding-left: 0;
      }

      .search__arrow {
        width: 9px;
        height: 5px;
        right: 12px;
      }

      .people {
        padding-bottom: 0;
      }

      .people__item {
        border-color: #a1a3a5;
      }

      .people__plus,
      .people__minus {
        stroke: #c7c8cb;
      }

      input.search__input {
        border: 1px solid #dddddd;
        padding-top: 14px;
        padding-bottom: 14px;
        font-weight: 700;
      }
    }
input.search__input
  border: none
  font-weight: 700
  color: #040404
  padding-left: 0
  padding-right: 0
  padding-left: 50px
  padding-right: 25px
  padding-top: 9px
  padding-bottom: 9px
  cursor: pointer
  placeholderColor(#040404)
  +b(mobile)
    padding-left: 37px; 
    
// autocomplete
//----------------------------------------------------//
.ui-autocomplete
  display: none
  position: absolute
  width: 100%
  min-width: 180px
  border-radius: 10px
  box-shadow: 0 0 3.8px 0.2px rgba(35, 41, 50, 0.3)
  background: #fff
  font-size: 14px
  color: #010101
  .scroll-wrap
    width: 100%
    padding-top: 20px
    padding-bottom: 20px
  .scroll-panel
    width: 100% !important
    max-height: 155px
  .ui-menu-item
    padding: 0 18px 0 48px
    cursor: pointer
    &:hover
      color: #24b3fd
.ui-helper-hidden-accessible
  display: none
// drops
//----------------------------------------------------//
.people-drop
  display: none
  width: 295px
  position: absolute !important
  z-index: 100
  padding: 0 !important
  &.active
    display: block
  &.clone
    display: block !important
    position: static !important
    box-shadow: none !important
    background: transparent !important
    margin: 0 auto 15px
    .dropdown__triangle
      display: none
  +b(mobile)
    width: 100%;
    display: block !important;
    top: 45px !important;
    left: 0 !important;
    position: absolute !important;
    bottom: 0;
    border-radius: 0 !important;
    transform: translateX(-100%);
    transition: all 0.2s;
    box-shadow: none !important;

    .dropdown__triangle {
      display: none;
    }

    &.active {
      transform: translateX(0);
    }
.calendar-drop
  display: none
  width: 558px
  position: absolute !important
  z-index: 100
  padding: 0 !important
  &.active
    display: block
  +b(mobile)
    padding-top: 5px;

    &__item {
      padding: 10px 45px 3px 0;

      &:last-child {
        padding: 10px 0 3px 45px;
      }
    }

    &__top {
      margin-bottom: 8px;
    }
// people
//----------------------------------------------------//
.people
  clearfix()
  padding: 20px 0 15px
  &__item
    float: left
    width: 50%
    border-right: 1px solid #e8e8ea
    padding: 0 30px 0 20px
    text-align: center
    &:last-child
      padding: 0 20px 0 30px
      border-right: none
  &__top
    padding: 0 20px
    height: 45px
    position: relative
    .icon-baby
      margin-top: 4px
      margin-bottom: 4px
  &__plus,
  &__minus
    position: absolute
    top: 50%
    transform: translateY(-50%)
    cursor: pointer
    &:hover
      stroke: #24b3fd
  &__minus
    left: 0
  &__plus
    right: 0
  &__value
    display: block
  +b(mobile)
    padding: 18px 25px 25px;

    &__title {
      margin-bottom: 8px;
    }

    &__left,
    &__right {
      width: 100%;
      float: none;
      text-align: center;
      padding: 0 20px;
    }

    &__left {
      margin-bottom: 14px;
    }

    &__datepicker {
      padding-top: 5px;
      margin-bottom: 25px;
    }

    &__button {
      display: block;
    }

  
input.people__input
  border: none
  display: inline-block
  vertical-align: top
  width: 18px
  background-color: transparent
  padding: 0 !important
  line-height: 1
// calendar
//----------------------------------------------------//
.calendar
  padding: 10px 40px 25px
  &__left,
  &__right
    float: left
    width: 50%
  &__left
    padding-right: 25px
  &__right
    padding-left: 15px
  &__top
    clearfix()
    margin-bottom: 16px
  &__title
    display: block
    color: #24b3fd
    text-align: center
    margin-bottom: 15px
  &__amount
    position: relative
    padding: 0 33px
  &__minus,
  &__plus
    width: 33px
    text-align: center
    position: absolute
    top: 0
    height: 31px
    cursor: pointer
    border: 1px solid #dddddd
    [class^="icon-"],
    [class*="icon-"]
      position: absolute
      top: 50%
      left: 50%
      transform: translate(-50%, -50%)
    &:hover
      [class^="icon-"],
      [class*="icon-"]
        stroke: $secondFontColor
  &__minus
    left: 0
    border-right: none
  &__plus
    right: 0
    border-left: none
  &__button
    display: none
    min-width: 180px
    margin: 0 auto
    padding-top: 12px
    padding-bottom: 12px
  +b(mobile)
    color: #000000;
input.calendar__nights
  text-align: center
  border-color: #dddddd
  height: 31px
  font-size: 16px
// ui-datepicker
//----------------------------------------------------//
.ui-datepicker-inline
  clearfix()
.ui-datepicker-group
  float: left
  width: 50%
  padding: 0 15px
.ui-datepicker-title
  font-weight: 700
  color: #24b3fd
  text-align: center
  margin-bottom: 5px
  +b(mobile)
    td {
      padding: 1px;
    }
.ui-datepicker-calendar
  text-align: center
  thead
    font-size: 12px
    text-transform: uppercase
    color: #cacaca
  a
    display: inline-block
    width: 22px
    height: 22px
    line-height: 22px
    text-align: center
    color: #323131
    text-decoration: none
    &:hover
      color: #24b3fd
  +b(mobile)
    display: block;
.ui-datepicker-today
  a
    color: #24b3fd
.ui-datepicker-current-day
  a
    background-color: #24b3fd
    color: #fff
    border-radius: 50%
    &:hover
      color: #fff
.ui-datepicker-prev,
.ui-datepicker-next
  width: 7px
  height: 13px
  font-size: 0
  &:hover
    opacity: .8
.ui-datepicker-prev
  float: left
  margin-left: -10px
  background-image: url("../images/arrow-left.svg")
.ui-datepicker-next
  float: right
  margin-right: -10px
  background-image: url("../images/arrow-right.svg")
// value
//----------------------------------------------------//
.value
  display: none
  font-size: 12px
  line-height: 1.4
  position: relative
  padding-right: 30px
  margin-bottom: 8px
  &__edit
    position: absolute
    right: 0
    top: 8px
    cursor: pointer
  &__arrow
    vertical-align: middle
    margin: 0 20px 0 13px
  &__value
  +b(mobile)
    display block
// controls
//----------------------------------------------------//
.controls
  display: none
  position: fixed
  z-index: 90
  bottom: 20px
  width: 100%
  text-align: center
  transition: all 0.2s
  &.hidden
    opacity: 0
    visibility: hidden
  &__button
    min-width: 130px
    padding: 12px 15px
    margin: 0 10px
    &--sort
      display: none
  +b(tablet)
    display: block;
  +b(mobile)
    &__button {
      &--sort {
        display: inline-block;
      }
    }
#facebook
  .ajcls-2
    fill: #3a559f !important
#yandex-money
  fill: #fff !important
//----------------------------------------------------//
// MAIN                                               //
//----------------------------------------------------//
.temprow
  margin: 0 0 30px 0
  position: relative
  &.dark
    background: $darkColor
.columns
  padding: 10px 15px 0
.center-text
  text-align: center
  padding: 0 0 30px 0
.small-text
  p
    font-size: 12px
    line-height: 16px
// nav ------------------------------
.nav
  //nav list
  &__list
    display: table
    width: 100%
    &-item
      display: table-cell
      text-align: center
      &:first-child
        text-align: left
      & *
        text-align: left
  &__link
    font-size: 16px
    font-weight: 600
    color: #0e0e0e
    text-decoration: none
    &:hover
      color: $secondFontColor
    line-height: 51px
    height: 51px
    display: inline-block
  //nav sign in
  &__sign
    &-icon
      width: 22px
      height: 26px
      fill: $secondFontColor
      margin: 0 12px 0 0
      vertical-align: middle
    &-enter
      display: none
    &-link
      text-decoration: none
      display: inline-block
      vertical-align: middle
      line-height: 26px
      &:hover
        color: $darkBlueColor
        .nav__sign-icon
          fill: $darkBlueColor
      &--exit
        margin: 0 0 0 7px
        padding: 0 0 0 12px
        border-left: 1px solid #d1d1d1
// nav end ------------------------
//dropdown ---------------------------
.dropdown
  &__box
    padding: 25px
    border-radius: 10px
    box-shadow: 0 0 3.8px 0.2px rgba(35, 41, 50, 0.3)
    position: absolute
    background: #fff
    clearfix()
    top: 100%
    z-index: 999
  &__triangle
    width: 20px
    height: 12px
    position: absolute
    bottom: 100%
    left: 50%
    margin: 0 0 0 -10px
    overflow: hidden
    &:before
      content: ''
      display: block
      width: 12px
      height: 12px
      transform: rotate(45deg) skew(10deg, 10deg)
