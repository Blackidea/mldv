@import '../base/mixins&includes.styl'

.calendarSlider
  position relative
  &:before
    content ''
    position absolute
    border-top 1px solid #cccccc
    width 100%
    bottom 50px
    left 0
  &:after
    content ''
    position absolute
    border-top 1px solid #cccccc
    width 100%
    bottom 69px
    left 0
  &__arrow
    position absolute
    height 20px
    width 20px
    & > svg
      fill #457086
      height 100%
      width 100%
    &.__left
      top 50%
      left 0
    &.__right
      top 50%
      right 0
        
.calendar-price
  margin 30px 0

  +b(mobile)
    display none

  & > .container
    display: flex
    align-items: center

  &__btn
    cursor: pointer

    & > svg
      width: 8px
      height: 15px
      fill: #677e8a

    &_next
      order: 3
      margin-left: 10px

    &_prev
      order: 1
      margin-right: 10px

  & .swiper-container
    order: 2

#mapPrice
  max-height: 100%
  max-width: 100%
  height 500px
  width 900px
  height:400px
  width:100%
  border-radius:12px

.hotelsTabs
  margin-top: 35px

  +b(mobile)
    margin-top: 22px

  &__bonus
    clearfix()
  &__block
    height:auto
    width:100%
    // background:#000
    +b(mobile)
      height auto
  &__nav
    padding 25px 0
    margin-bottom 50px
    border-top 1px solid #9f9fa0
    border-bottom 1px solid #9f9fa0
    +b(mobile)
      display none

    & > .ui-tabs-active > a
      color #24b3fd !important
      font-weight 600
      font-size 22px

      &:after
        position absolute
        content url('../images/icon/arrow-down.png')
        height 100%
        width 80%
        top 25px
        left 50%
        transform translate(-50%, 50%)
        background #fff
        +b(mobile)
          background none
          left 90%
          top 0
          width 20px
          height 11px
          transform translate(0, 0)


    & > li
      position relative
      display inline-block
      width 24%
      text-align center
      font-size 22px
      font-weight 300
      +b(mobile)
        width 100%
      & > a
        text-decoration none
        color #677e8a !important
        height 20px
        width 20px
  
        &:hover
          color #24b3fd !important
          font-weight 600
          font-size 22px
    
          &:after
            position absolute
            content url('../images/icon/arrow-down.png')    
            height 100% 
            width 80%
            top 25px
            left 50%
            transform translate(-50%, 50%)
            background #fff
            +b(mobile)
              background none
              left 90%
              top 0
              width 20px
              height 11px
              transform translate(0, 0)
.hotelsDescription
  margin-top: 33px
  clearfix()

  +b(mobile)
    margin-top: 23px

  &__accordTitle
    position relative
    display none
    text-align center
    font-size 22px
    font-weight 300

    +b(mobile)
      cursor pointer
      width 100%
      display flex
      justify-content space-between
      text-decoration none
      color #677e8a !important
      font-size: 18px
      font-family: "Open Sans Light", "Open Sans", sans-serif;
      font-weight: 300
      border-bottom: 1px solid #c6c6c6
      padding: 8px 0 9px
      transition: all .25s ease-in-out

      &:first-of-type
        border-top: 1px solid #c6c6c6

      &::after
        content url('../images/icon/arrow-down-sm.png')
        background none
        width 20px
        height 11px
        transform translate(0, 0)

      &:hover
        color #24b3fd !important


      .active&
        font-weight: 700
        color #24b3fd !important
        border-bottom: 1px solid transparent

        &::after
          content url('../images/icon/arrow-down.png')
          position: absolute
          top: 50%
          right: 0
          width 20px
          height 11px
          transform: translate(0, 0) rotate(180deg)

  &__column
    col(1/3)
    +b(mobile)
      unstack()

  &__heading
    display: flex

  &__icon
    display block
    height 30px
    width 30px
    & > svg      
      height 100%
      width 100%
      fill #07405e

    &-wrapper
      display: block
      height: 100%

  &__title
    display block
    font-size 20px
    color #07405e
    padding-left 10px
    align-self: center
    +b(mobile)
      font-size 18px
      text-transform uppercase

  &__value
    margin 20px 0
    font-size 14px
    font-weight 400
    +b(mobile)
      margin 12px 0 3px
  &__name 
    font-weight 600
  &__list
    margin 20px 0
    +b(mobile)
      margin 13px 0 15px
  &__li
    position relative
    font-size: 14px
    padding-left: 20px
    font-weight: normal
    +b(mobile)
      margin-top 5px
      padding-left 13px
      &:first-of-type
        margin-top 0
    // padding-top: 10px
    // padding-bottom: 10px
  &__li:before
    position absolute
    top 8px
    left 0
    color: #999
    content: ''
    height: 5px
    width: 5px
    background: #677e8a
    border-radius: 53px
  &__info
    +b(mobile)
      margin-top 34px
      &:first-child
        margin-top 0
  
.hotelsBonus

  &__column
    col(1/2)
    display block
    position: relative
    border-radius 12px
    height 400px
    color #fff    
    text-decoration none
    padding 60px 60px 0
    transition all .25s ease-in-out
    overflow hidden
    
    &:hover
      box-shadow: 0px 0px 9.5px 0.5px rgba(35, 41, 50, 0.3)
      .hotelsBonus__hover
        opacity 1
        pointer-events all

    +b(mobile)
      unstack()
      padding 20px 20px
      margin-top 10px
      margin-bottom 10px
      height auto
      width 100%
      min-height 400px
      &:hover
        box-shadow: none
        .hotelsBonus__hover
          opacity 0
          pointer-events none

  &__image
    position: absolute
    top: 0
    right: 0
    bottom: 0
    left: 0
    margin: auto
    min-width: 100%
    min-height: 100%
    width: 0
    object-fit: cover

  &__content
    position: relative
    margin: auto
    display: block
    z-index: 1
  
  &__hover
    position absolute
    left 0
    top 0
    width 100%
    height 100%
    background-color #fff
    z-index 2
    padding 39px 24px
    opacity 0
    pointer-events none
    transition all .25s ease-in-out
      
    p
      padding 0
      
    &-heading
      font-size 22px
      font-weight 600
      color #07405e
      text-transform uppercase
      line-height 1.2em
      letter-spacing 0.02em
        
    &-optionlist
      margin-top 31px
      font-size 14px
      color #333333
      letter-spacing 0.02em
        
      li
        margin-top 14px
        &:first-child
          margin-top 0
          
    &-optionname
      font-weight 600
      color #000
      margin-right 5px
      
    &-attention
      border-top 1px solid #c6c6c6
      margin-top 16px
      padding-top 13px
      
    &-attentionheading
      font-size 14px
      font-weight 600
      color #e13838
      letter-spacing 0.02em
      
    &-attentiontext
      font-size 14px
      color #333333
      margin-top 2px
      line-height 1.5em
        
    &.popup
      position relative
      padding 0 15px 35px
      opacity 1
      pointer-events all
      background-color transparent
      margin-top -10px
      .hotelsBonus__hover-heading
        color #24b3fd
        text-transform none
        letter-spacing -0.04em
        text-align center
      .hotelsBonus__hover-optionlist
        margin-top 20px
        

  &__name
    text-align center
    font-weight bold    
    font-size 22px
    margin-bottom 24px
    line-height 30px
    text-transform uppercase
    
    +b(mobile)
      font-size 18px
      text-transform uppercase
  &__info
    text-align center
    margin-bottom 25px
    font-size 14px  
        
  &__border-bottom
    height 1px
    width 30%
    border-top 1px solid #fff
    display block
    margin 0 auto
  &__date
    margin-top 20px
    font-size 22px
    &-name
      font-size 12px
      text-align center
      padding 0
    &-value
      margin-top 20px
      font-size 22px
      text-align center
      padding 0
.filterHotels
  &__switch-inner
    background none
    margin-top: 36px
  &__switch-view
    border 1px solid #fff
    // border-radius 4px
      
.filterMob
  &__search
    +b(mobile)
      float none !important
      margin-top 10px !important
      width 100% !important
      margin-bottom 10px !important
      background-color #fff !important
      border 1px solid #999 !important
      border-left 1px solid #999 !important
      border-radius 8px !important
    
.holidayCreate
  margin-top: 300px

  +b(mobile)
    margin-top 18px
    padding-bottom 15px

    & > .section__title .h2
      font-family: Roboto, "Open Sans"
      font-weight 700
      font-size: 22px
      line-height: 1.35em
      padding: 0 35px

  &__filterMob, .hotelsTours__filterMob
    position fixed
    top 0
    padding-top 47px
    z-index 300
    height 100%
    width 100%
    background #fff

  &__filter
    position: relative
    min-height 200px
    margin-top: 20px
    margin-bottom: 10px

    +b(mobile)
      background none
      height auto
      display none

    &::after
      content: ''
      position: absolute
      top: 0
      left: 0
      display: block
      width: 100%
      height: 100%
      background-color: rgba(0, 0, 0, .43)
      z-index: 2

    &-image
      position: absolute
      top: 0
      right: 0
      bottom: 0
      left: 0
      margin: auto
      min-width: 100%
      min-height: 100%
      width: 0
      object-fit: cover
      z-index: 1

    &-content
      position: relative
      display: block
      padding-top: 17px
      padding-bottom: 17px
      z-index: 3

      & > .filter__box-shadow
        box-shadow: none
      & > .filterHotels__switch-inner
        margin-top: 0

        & > .filterHotels__switch-view
          border: 0

  &__selectors
    overflow hidden
    width 50%
    margin 20px auto 20px

  &__selectors-item
    position relative
    col(1/2)
    
    & > label
      position absolute
      height 9px
      width 27px
      right 10px
      top 50%
      transform translateY(-50%)
      & > svg
        display block
        width 100%
        height 100%
        fill #fff
    & > select
      border-radius 5px
      height 40px  
      color #fff
      background rgba(9,60,87,.5)
      outline none
      border: 1px solid #fff

  &__sort-inner
    text-align center
    padding-top 12px
    border-top 1px solid rgba(204, 204, 204, .5)
    margin 0 auto

    .catalog-sort__list
      margin-right: 0
      padding-right: 0

    .catalog-sort__title, .catalog-sort__link
      color: #ffffff

    .catalog-sort__link:hover
      color: #ffffff

    .catalog-sort__list-item.active .catalog-sort__link
      color: #677e8a

  &__result
    +b(mobile)
      display none
  &__sliderMob
    display none
    +b(mobile)
      display block
  &__slider
    position relative
    margin-top 20px
    margin-bottom 30px
    min-height 700px
    +b(mobile)
      min-height auto
      margin-top 22px
      margin-bottom 0

    &__arrow
      position absolute
      height 40px
      width 30px
      background #457086
      & > svg
        padding 10px 5px
        fill #fff
        height 100%
        width 100%
      &.__left
        top 45%
        left 0
      &.__right
        top 45%
        right 0

.numbersSlider
  &__item
    position: relative
    display block
    border-radius 10px
    margin 0 15px 10px
    overflow hidden
    box-shadow: 0 0 3.8px 0.2px rgba(35, 41, 50, 0.3)
    transition all 0.3s ease-in-out 0s
    transform translate3d(0,0,0)

    +b(mobile)
      margin: 30px 0 0
      &:first-child
        margin-top 0
      &:nth-child(n+4)
        display none

    &:hover
      cursor pointer
      box-shadow: 0 0 9.5px 0.5px rgba(35, 41, 50, 0.3)

    &:hover .numbersSlider__image
      max-height: 145px

      +b(mobile)
        transition none
        max-height 115px

    &:hover .numbersSlider__zoom
      display: block
      opacity: 1

      +b(mobile)
        display: none

  &__zoom
    position: absolute
    top: 114px
    right: 14px
    display: block
    width: 20px
    height: 20px
    z-index: 3
    opacity: 0
    transition: opacity .5s ease-in-out

    +b(mobile)
      display: none

  &__lunch
    position: absolute
    top: 83px
    right: 24px
    display: none
    width: 24px
    height: 22px
    z-index: 3
    fill: #fff

    +b(mobile)
      display: block

  &__images
    position relative
    overflow hidden 

    &::after
      content: ''
      position: absolute
      top: 0
      left: 0
      display: block
      width: 100%
      height: 100%
      background-color: rgba(0, 0, 0, .25)

      +b(mobile)
        top 35%
        height: 65%
        background-color: transparent
        background-image: linear-gradient(transparent, #07405e)
        opacity: .86

  &__image
    width: 263px
    height: auto
    max-height 115px
    object-fit: cover
    z-index 2
    transform: translateZ(0)
    transition all .3s ease-in-out

  &__arrow
    position absolute
    z-index 10
    height 20px    
    width 20px
    & > svg
      width 100%
      height 100%
      fill #fff
    &.__left
      top 50%
      left 10px
    &.__right
      top 50%
      right 10px

  &__type
    display: flex
    flex-direction: column
    justify-content center
    margin 0 15px
    padding: 13px 0
    border-bottom 1px solid #999
    height 85px

    +b(mobile)
      border-bottom: none
      margin 0 23px
      height 74px

  &__info
    display: flex
    flex-direction: column
    justify-content center
    margin 0 15px
    padding: 13px 0
    border-bottom 1px solid #999
    height 85px

    +b(mobile)
      display none

      &_price
        display: block
        margin: 0
        padding: 0
        height: 0
        border: 0

        & ^[0]__propertie
         display: none

  &__propertie
    margin-bottom: 4px
    font-size 12px
    color #999999
  &__value
    font-size 14px
    font-weight bold
    color #000
  &__price
    font-size 25px
    font-weight bold
    color #24b3fd

    +b(mobile)
      position: absolute
      top: 105px
      left: 24px
      transform: translateY(-100%)
      font-size: 22px
      font-weight: normal
      color: #ffffff

  &__form
    padding-top 20px
    margin 0 15px
    height 65px

    +b(mobile)
      display: none

  &__checkbox
    display inline-block
    vertical-align middle
    background #fff
    border 1px solid #999
    border-radius 80px
    height 15px
    width 15px 
    transition all 0.3s ease-in-out 0s

  &__labelCheckbox
    display inline-block
    vertical-align middle
    font-size 16px
    padding-left 10px

.checkboxActive
  background #24b3fd
  border 1px solid #24b3fd
  transition all 0.3s ease-in-out 0s

  & + .numbersSlider__labelCheckbox
    text-indent -9999px
    position: relative

    &::after
      position: absolute
      top: 0
      left: 0
      display inline-block
      width: 200px
      content: attr(data-active-text)
      text-indent 0
      padding-left 10px

.numberResult
  position: relative
  border-radius: 10px
  box-shadow: 0 0 3.8px 0.2px rgba(35, 41, 50, 0.3)
  overflow: hidden
  display: flex
  justify-content center
  align-items center
  min-height: 230px
  width: 100%

  &__image
    position: absolute
    top: 0
    right: 0
    bottom: 0
    left: 0
    margin: auto
    min-width: 100%
    min-height: 100%
    width: 0
    object-fit: cover
    opacity: .2
    z-index: 2

    &-wrapper
      position: absolute
      top: 0
      left: 0
      display: block
      width: 100%
      height: 100%
      background-color: #1c0d00
      z-index: 1

      &::after
        content: ''
        position: absolute
        top: 0
        left: 0
        display: block
        width: 100%
        height: 100%
        background-image: linear-gradient(to bottom, #ffffff, #ffc600)
        opacity: .2

  &__content
    margin: 15px
    display: inline-block
    position: relative
    z-index: 3
    +below(1220px)
      width: 95%

  &__sumBonus, &__paymentMethods
    display: flex

  &__sum
    color #fff
    width 225px
    display inline-block
    vertical-align middle
    +b(tablet)
      width 27%
    &Title
      color #fff
      font-size 14px
    &Value
      font-size 60px
      font-weight bold
      line-height normal
    &Icon
      display inline-block
      vertical-align middle
      height 35px
      width 30px
      & > svg
        width 100%
        height 100%
        fill #fff
    &Text 
      display inline-block
      vertical-align middle
      padding-left 15px
    &Link
      font-size 13px
      outline none
      color #fff
  &__payment
    display inline-block
    vertical-align middle 
    margin-left 47px
    border-left 1px solid #999
    border-right  1px solid #999
    padding-left 50px
    color #fff
    font-size 13px
    line-height 18px
    width 440px
    +b(tablet)
      width 50%
      padding-left 1%
      margin-left 2% 
    &Link
      display block
      margin-top 23px
      margin-bottom 23px
      font-size 13px
      outline none
      color #fff      
    &Icon
      display inline-block
      vertical-align middle
      height 35px
      width 30px
      & > svg
        width 100%
        height 100%
        fill #fff
    &Text 
      display inline-block
      vertical-align middle
      padding-left 15px
  
  &__buy
    display inline-block
    vertical-align middle
    +b(tablet)
      width 20%
    &Button 
      color #fff
      margin-left 45px
      width 260px
      +b(tablet)
        width 100%
        margin-left 0
      
.hotelsTours
  margin-top: 80px
  +b(mobile)
    margin-top 19px

  & .hotelsToursfilter__buttonMob
    display: none

    +b(mobile)
      display: block
