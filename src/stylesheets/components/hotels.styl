    &Switch
      display inline-block
      vertical-align middle
      width 400px
      padding-left 30px
      +below(1220px)
          display block
          margin 20px auto 0
      +b(mobile)
        display none
    &Title
      display inline-block
      vertical-align middle
      font-weight bold
      color #000
      padding-right 30px
    &List
      display inline-block
      vertical-align middle
    &ListItem
      display inline-block
      vertical-align middle
      & > a
        text-decoration none
        color #999
        padding 0 10px
        border-radius 12px
      &.active > a
        background #f4f4f4
        color #000
  &__tableMob
    margin-top: 20px
    display none
    +b(mobile)
      display block
      margin-top 22px
  &__table
    +b(mobile)
      display none
    &Titles
      display table
      height 50px
      border-top 1px solid rgba(204, 204, 204, .5)
      border-bottom 1px solid rgba(204, 204, 204, .5)
      color #999
    &Title
      display inline-block
      vertical-align middle
      display table-cell
      &:last-child
        text-align right
      &.__tour
        width 340px
      &.__duration
        width 195px
      &.__numbers
        width 200px
      &.__food
        width 161px
      &.__airCompanie
        width 162px
      &.__price
        width 58px
    &Cells
      display table
      padding-top 10px
    &Cell
      display table-cell
      vertical-align middle
      padding-top 30px
      padding-bottom 30px
      font-size 14px
      &:last-child
        text-align right
      &.__tour
        width 340px
      &.__duration
        width 195px
      &.__numbers
        width 200px
      &.__food
        width 161px
      &.__airCompanie
        width 162px
      &.__price
        width 61px
        color #24b3fd
        font-size 18px
        font-weight bold
    
.hotelsAround
  margin-top 40px
  margin-bottom 40px
  +b(mobile)
    display none
  &__title
    margin-bottom 50px
.sliderHotelsAround
  position relative
  height 357px
  +below(1220px)
    height 380px
  +b(mobile)
    height auto
  .slick-dots
    bottom: auto
    top: 100%
    margin-top: 15px
    button
