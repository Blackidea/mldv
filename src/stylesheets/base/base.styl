@import 'jeet'
@import 'nib'
@import 'config'
@import 'mixins'
// @import 'mixins&includes.styl'
@import 'placeholders'
@import 'normalize.styl'
// Animation library "animation.css": fadein, slidein and etc
@import '../../../node_modules/animate.css/animate.min.css'

// edit()
.clearfix
  clearfix()
.row
  height inherit
  

html
  box-sizing border-box  
  background-color #fff
  font-size $baseFontSize

body
  font-family: 'GothamProLight';
a
  text-decoration none
  color inherit
  outline none
  &:visit,:hover,:link,:active
    color inherit
    text-decoration none
    
h1, h2, h3, h4 {
    color: #283172;
}

h2
  font-weight 300

ul li 
  list-style none
    
